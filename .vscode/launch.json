{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Visual-Assistant",
            "request": "launch",
            "args": [
                "--no-sound-null-safety"
             ],
             "codeLens": {
                "for": [
                   "run-test",
                   "run-test-file",
                   "debug-test",
                   "debug-test-file",
                ],
                "title": "${debugType} (--no-sound-null-safety)"
             },
            "type": "dart"
        },
        {
            "name": "Visual-Assistant (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "Visual-Assistant (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}